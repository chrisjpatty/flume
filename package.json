    "@typescript-eslint/parser": "2.x",
    "babel-core": "^6.26.3",
    "babel-eslint": "10.x",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.4",
    "cypress": "^4.9.0",
    "eslint": "6.x",
